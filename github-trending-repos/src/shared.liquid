{% if trmnl.plugin_settings.custom_fields_values.lang != "" %}{% assign hasLang = true %}{% endif %}
<div class="layout">
  <div class="columns" data-overflow-max-cols="{{ maxCol | default: 2 }}">
    <div class="column">
      {% for repo in data %}
      {% assign path = repo.repo | split: "/" %}
      <div class="item">
        <div class="meta">
          <span class="index">{{ forloop.index }}</span>
        </div>
        <div class="{{ contClass | default: "content w--full" }}">
          <span class="{{ titleClass | default: "title title--small" }}"><span class="text--gray-30">{{ path[1] }} /</span> {{ path[2] }}</span>
          <span class="{{ descClass | default: "description" }}">{{ repo.desc }}</span>
          <div class="{{ statsClass | default: "flex gap--distribute w--full" }}">
            <div class="flex gap">
            {% unless hasLang %}
            <span class="label label--small label--underline gap--xsmall">
              <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 16 16">
                <path fill="currentColor" d="M8 14A6 6 0 1 0 8 2a6 6 0 0 0 0 12" />
              </svg>
              {{ repo.lang }}
            </span>
            {% endunless %}
            <span class="label label--small label--underline gap--xsmall">
              <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 16 16">
                <path fill="currentColor" d="M7.194 2.101a.9.9 0 0 1 1.614 0l1.521 3.082l3.401.495a.9.9 0 0 1 .5 1.535l-2.462 2.399l.581 3.387a.9.9 0 0 1-1.306.949l-3.042-1.6l-3.042 1.6a.9.9 0 0 1-1.306-.949l.58-3.387l-2.46-2.4a.9.9 0 0 1 .499-1.534l3.4-.495zM8 2.726L6.546 5.673a.9.9 0 0 1-.677.492l-3.253.473L4.97 8.932a.9.9 0 0 1 .258.797l-.555 3.24l2.91-1.53a.9.9 0 0 1 .837 0l2.91 1.53l-.556-3.24a.9.9 0 0 1 .258-.797l2.354-2.294l-3.253-.473a.9.9 0 0 1-.677-.492z" />
              </svg>
              {{ repo.stars }}
            </span>
            <span class="label label--small label--underline gap--xsmall">
              <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 16 16">
                <path fill="currentColor" d="M5 5.95a2.5 2.5 0 1 0-1 0v4.1a2.5 2.5 0 1 0 1 0V9h4.5A2.5 2.5 0 0 0 12 6.5v-.55a2.5 2.5 0 1 0-1 0v.55A1.5 1.5 0 0 1 9.5 8H5zM4.5 5a1.5 1.5 0 1 1 0-3a1.5 1.5 0 0 1 0 3m7 0a1.5 1.5 0 1 1 0-3a1.5 1.5 0 0 1 0 3M6 12.5a1.5 1.5 0 1 1-3 0a1.5 1.5 0 0 1 3 0" />
              </svg>
              {{ repo.forks }}
            </span>
            </div>
            <span class="label label--small label--underline gap--xsmall">
              <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 16 16">
                <path fill="currentColor" d="M7.194 2.101a.9.9 0 0 1 1.614 0l1.521 3.082l3.401.495a.9.9 0 0 1 .5 1.535l-2.462 2.399l.581 3.387a.9.9 0 0 1-1.306.949l-3.042-1.6l-3.042 1.6a.9.9 0 0 1-1.306-.949l.58-3.387l-2.46-2.4a.9.9 0 0 1 .499-1.534l3.4-.495z" />
              </svg>
              {{ repo.change }} today
            </span>
          </div>
        </div>
      </div>
      {% endfor %}
    </div>
  </div>
</div>

<div class="title_bar">
  <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" class="image image-stroke">
    <path fill="currentColor" d="M12 .297c-6.63 0-12 5.373-12 12c0 5.303 3.438 9.8 8.205 11.385c.6.113.82-.258.82-.577c0-.285-.01-1.04-.015-2.04c-3.338.724-4.042-1.61-4.042-1.61C4.422 18.07 3.633 17.7 3.633 17.7c-1.087-.744.084-.729.084-.729c1.205.084 1.838 1.236 1.838 1.236c1.07 1.835 2.809 1.305 3.495.998c.108-.776.417-1.305.76-1.605c-2.665-.3-5.466-1.332-5.466-5.93c0-1.31.465-2.38 1.235-3.22c-.135-.303-.54-1.523.105-3.176c0 0 1.005-.322 3.3 1.23c.96-.267 1.98-.399 3-.405c1.02.006 2.04.138 3 .405c2.28-1.552 3.285-1.23 3.285-1.23c.645 1.653.24 2.873.12 3.176c.765.84 1.23 1.91 1.23 3.22c0 4.61-2.805 5.625-5.475 5.92c.42.36.81 1.096.81 2.22c0 1.606-.015 2.896-.015 3.286c0 .315.21.69.825.57C20.565 22.092 24 17.592 24 12.297c0-6.627-5.373-12-12-12" />
  </svg>
  <span class="title">{{ trmnl.plugin_settings.instance_name }}</span>
  <span class="instance" style="text-transform: capitalize;">
    {{ trmnl.plugin_settings.custom_fields_values.duration }}
    {% if hasLang %}
    ({{ langMap[trmnl.plugin_settings.custom_fields_values.lang ] | default: trmnl.plugin_settings.custom_fields_values.lang }})
    {% endif %}
  </span>
</div>
