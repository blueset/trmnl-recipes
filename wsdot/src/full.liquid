{% if view == 'travelTime' %}
  {%
    render 'trafficTime',
    trafficTime: trafficTime,
    trmnl: trmnl
  %}
{% elsif view == 'alert' %}
  {%
    render 'alerts',
    alerts: alerts,
    trmnl: trmnl
  %}
{% elsif view == 'camera' %}
  {%
    render 'cameras',
    cameras: cameras,
    cameraTitleClass: 'title title--small h-[2lh] content-end',
    trmnl: trmnl
  %}
{% endif %}