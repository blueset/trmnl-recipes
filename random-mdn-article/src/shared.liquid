<style>
  .chip {
    border-radius: 9999px;
    border: 1px solid black;
    height: 35px;
    padding: 5px;
  }
  .pair {
    display: inline-flex;
    height: 24px;
    .image-stroke {
      z-index: 1;
    }
    > div:last-child {
      margin-inline-start: -6px;
    }
    &.no path {
      opacity: 0.5;
    }
  }
  del {
    text-decoration-color: #777;
  }
  .screen--lg .mdn {
    --factor: 1.5;
    --richtext-small-font-size: calc(13px * var(--factor) * var(--ui-scale));
    --richtext-small-line-height: calc(18px * var(--factor) * var(--ui-scale));
    --richtext-font-size: calc(16px * var(--factor) * var(--ui-scale));
    --title-font-size: calc(21px * var(--factor) * var(--ui-scale));
  }
  .screen--4bit .mdn {
    .richtext .content--small {
      --richtext-small-font-weight: 400;
    }
    .title.page-title {
      --title-font-weight: 600;
    }
  }
</style>

{% template yes %}
<div class="bg--black" style="width: 24px; height: 24px; mask-image: url('data:image/png;base64,')"></div>
{% endtemplate %}
{% template no %}
<div class="bg--gray-30" style="width: 24px; height: 24px; mask-image: url('data:image/png;base64,')"></div>
{% endtemplate %}
{% template chips %}
<div class="chip flex flex--row gap--xsmall">
  <div class="pair">
    <div class="image-stroke"><div class="{% if baseline.support.chrome and baseline.support.chrome_android %}bg--black{% else %}bg--gray-30{% endif %}" style="width: 24px; height: 24px; mask-image: url('data:image/png;base64,')"></div></div>
    {% if baseline.support.chrome and baseline.support.chrome_android %}{% render 'yes' %}{% else %}{% render 'no' %}{% endif %}
  </div>
  <div class="pair">
    <div class="image-stroke"><div class="{% if baseline.support.edge %}bg--black{% else %}bg--gray-30{% endif %}" style="width: 24px; height: 24px; mask-image: url('data:image/png;base64,')"></div></div>
    {% if baseline.support.edge %}{% render 'yes' %}{% else %}{% render 'no' %}{% endif %}
  </div>
</div>
<div class="chip flex flex--row gap--xsmall">
  <div class="pair">
    <div class="image-stroke"><div class="{% if baseline.support.firefox and baseline.support.firefox_android %}bg--black{% else %}bg--gray-30{% endif %}" style="width: 24px; height: 24px; mask-image: url('data:image/png;base64,')"></div></div>
    {% if baseline.support.firefox and baseline.support.firefox_android %}{% render 'yes' %}{% else %}{% render 'no' %}{% endif %}
  </div>
</div>
<div class="chip flex flex--row gap--xsmall">
  <div class="pair">
    <div class="image-stroke"><div class="{% if baseline.support.safari and baseline.support.safari_ios %}bg--black{% else %}bg--gray-30{% endif %}" style="width: 24px; height: 24px; mask-image: url('data:image/png;base64,')"></div></div>
    {% if baseline.support.safari and baseline.support.safari_ios %}{% render 'yes' %}{% else %}{% render 'no' %}{% endif %}
  </div>
</div>
{% endtemplate %}
{% liquid
  assign link_size = link | size
  assign thresholds = "[17,32,53,78,106,134,154,192,230,271,321,367,425,458,520,586,644,718,792,858,929,1003,1091,1171,1273,1367,1465,1528,1628,1732,1840,1952,2068,2188,2303,2431,2563,2699,2809]" | parse_json
  assign module = 0
  for t in thresholds
    if link_size <= t
      assign module = forloop.index0 | times: 4 | plus: 21
      break
    endif
  endfor
%}
<script>
(() => {
  const root = document.currentScript.closest(".view");
  function repeat() {
    const pageTitle = root.querySelector(".page-title");
    if (!pageTitle) return requestAnimationFrame(repeat);
    console.log("root", root, "pageTitle", pageTitle);
    pageTitle.innerText = pageTitle.innerText.replace(/(?<=[a-z\.])(?=[A-Z])/ug, "â€‹");
  }
  requestAnimationFrame(repeat);
})();
</script>

      
