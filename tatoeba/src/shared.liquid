{% assign lang_data = lang_json | parse_json %}

{% liquid 
assign data = data[0]
assign authorNames = data.owner
assign licenses = data.license
assign main_language = trmnl.plugin_settings.custom_fields_values.main_language
assign translation_languages = trmnl.plugin_settings.custom_fields_values.translation_languages
assign romanization = trmnl.plugin_settings.custom_fields_values.romanization
assign jpn_display = trmnl.plugin_settings.custom_fields_values.jpn_display
assign cmn_display = trmnl.plugin_settings.custom_fields_values.cmn_display
assign sentences_per_translation_language = trmnl.plugin_settings.custom_fields_values.sentences_per_translation_language
assign approved_only = trmnl.plugin_settings.custom_fields_values.approved_only
%}
{% template sentence %}
{% assign bcp47 = lang_data[data.lang].bcp47 %}
{% if data.lang == "cmn" %}
  {% assign rendered_text = false %}
  {% for script in cmn_display %}
    {% liquid
      assign script_transcriptions = data.transcriptions | where: "script", script
      if approved_only == "yes"
        assign script_transcriptions = script_transcriptions | where: "needsReview", false
      endif
      assign transcriptions_size = script_transcriptions | size
    %}
    {% if transcriptions_size > 0 %}
      {% for t in script_transcriptions %}
        <div dir="auto" lang="{{ bcp47 }}-{{ t.script }}"{% if script == "Latn" and rendered_text %} class="title title--small"{% endif %}>{{ t.html | default: t.text }}</div>
        {% assign rendered_text = true %}
      {% endfor %}
    {% elsif data.script == script %}
      <div dir="auto" lang="{{ bcp47 }}-{{ data.script }}"{% if script == "Latn" and rendered_text %} class="title title--small"{% endif %}>{{ data.html | default: data.text }}</div>
      {% assign rendered_text = true %}
    {% endif %}
  {% endfor %}
  {% if rendered_text == false %}
    <div dir="auto" lang="{{ bcp47 }}-{{ data.script }}">{{ data.html | default: data.text }}</div>
  {% endif %}
{% elsif data.lang == "jpn" %}
  {% liquid
    assign script_transcriptions = data.transcriptions | where: "script", "Hrkt"
    if approved_only == "yes"
      assign script_transcriptions = script_transcriptions | where: "needsReview", false
    endif
    assign transcriptions_size = script_transcriptions | size 
  %}
  {% if jpn_display == "Hrkt" and transcriptions_size > 0 %}
    {% for t in script_transcriptions %}
      <div dir="auto" lang="{{ bcp47 }}-{{ t.script }}">{{ t.html | default: t.text }}</div>
    {% endfor %}
  {% else %}
    <div dir="auto" lang="{{ bcp47 }}">{{ data.html | default: data.text }}</div>
  {% endif %}
{% else %}
  {% liquid
    assign script_transcriptions = data.transcriptions | where: "script", "Latn"
    if approved_only == "yes"
      assign script_transcriptions = script_transcriptions | where: "needsReview", false
    endif
    assign transcriptions_size = script_transcriptions | size 
  %}
  {% if transcriptions_size > 0 %}
    {% if romanization == "both" or romanization == "no" %}
      <div dir="auto" lang="{{ bcp47 }}">{{ data.html | default: data.text }}</div>
    {% endif %}
    {% if romanization == "both" or romanization == "only" %}
      {% for t in script_transcriptions %}
        <div dir="auto" lang="{{ bcp47 }}-{{ t.script }}"{% if romanization == "both" %} class="title title--small"{% endif %}>{{ t.html | default: t.text }}</div>
      {% endfor %}
    {% endif %}
  {% else %}
    <div dir="auto" lang="{{ bcp47 }}">{{ data.html | default: data.text }}</div>
  {% endif %}
{% endif %}
{% endtemplate %}


