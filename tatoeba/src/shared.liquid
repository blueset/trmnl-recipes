<script type="module" data-t-script>
  import { applyPlaintext } from 'https://cdn.jsdelivr.net/gh/blueset/trmnl-tricks@2e95bbd620f0c5d0726e3dfbe4d765edb417ccf8/i18n.js';
  import { DefaultFunctions } from 'https://cdn.jsdelivr.net/npm/messageformat@4.0.0-13/lib/functions/+esm';

  const langJson = {{ lang_json }};
  
  applyPlaintext({
    langName: (context, options, input) => { 
      const result = (new Intl.DisplayNames(context.locales, { type: "language" })).of(input);
      const name = result !== input ? result : Object.values(langJson).find(a => a.bcp47 === result).name ?? result;
      return DefaultFunctions.string(context, options, name);
    },
    langNameList: (context, options, input) => { 
      const dn = new Intl.DisplayNames(context.locales, { type: "language" });
      const result = new Intl.ListFormat(context.locales[0], { style: "narrow" }).format(input.map(i => {
        const result = dn.of(i);
        const name = result !== input ? result : Object.values(langJson).find(a => a.bcp47 === result).name ?? result;
        return name;
      }));
      return DefaultFunctions.string(context, options, result);
    },
  });
</script>
